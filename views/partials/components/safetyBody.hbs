<div class="container safety-body pt-4">
    <div class="row">
        <div class="col-md-6">{{>components/blockAccount}}</div>
        {{#contains formType 'documents'}}
        <div class="col-md-6">{{>components/boxDocuments client=../client keyType='all' formType=../formType symbol=../symbol}}</div>
        {{else}}
        <div class="col-md-6">{{>components/blockKeyboard client=../client keyType='all' formType=../formType symbol=../symbol}}</div>
        {{/contains}}
        
    </div>
    {{>components/blockSecurity}}
</div>